@page
@using System.Reflection.Metadata
@model WebUi.Pages.Utilities.AddPage

@{
    ViewData["Title"] = "Регистрация услуги";
}

<!DOCTYPE html>

<html lang="ru">
<head>
    <title></title>
</head>
<body>
<div>
    <form method="post">
        <h4 class="m-3">Добавить новую услугу</h4>
        <div class="form-group m-3">
            <label for="inputUtilityName">Название услуги</label>
            <input name="utilityName" class="form-control" type="text" id="inputUtilityName" placeholder="Введите название услуги">
        </div>
        <div class="form-group m-3">
            <button type="submit" class="btn btn-primary">Добавить</button>
        </div>
    </form>
</div>
</body>
</html>